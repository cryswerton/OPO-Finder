{
    "manifest_version": 2,
    "name":"OPO Finder",
    "version":"1.0",
    "browser_action": {
		"default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}